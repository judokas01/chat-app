{
  "name": "@chat-app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\"",
    "compose:up": "docker-compose -f ./dev-docker/docker-compose.yml --profile local-mongo up --remove-orphans",
    "dev": "nx run-many -t serve",
    "code:check": "yarn _build:check && yarn _lint:check && yarn _prettier:check",
    "nest-generate": "sh -c 'yarn nx g @nx/nest:${0} ${1} --directory apps/chat-app/src/app/${1}'",
    "nest-gen:resource": "nx g @nx/nest:resource",
    "nest-gen:module": "nx g @nx/nest:module",
    "nest-gen:controller": "nx g @nx/nest:controller",
    "nest-gen:service": "nx g @nx/nest:service",
    "change-dir": "cd ./apps/chat-app/",
    "_prettier:check": "prettier --check \"apps/**/*.ts\"",
    "_lint:check": "nx run-many --target=lint",
    "_build:check": "nx run-many --target=build:check",
    "_with-test-env": "yarn env-cmd -f .env.test --silent",
    "check:build": "tsc --p apps/chat-app/tsconfig.app.json --noEmit",
    "test": "yarn _test:generate-gql && yarn _with-test-env vitest --run",
    "gql:gen": "yarn _test:generate-gql && yarn _test:generate-gql-types",
    "seed": "yarn _run apps/chat-app/src/scripts/seed-dev.ts",
    "_test:generate-gql": "yarn _run apps/chat-app/src/scripts/generate-gql-types.ts",
    "_test:generate-gql-types": "yarn graphql-codegen --config apps/chat-app/src/scripts/codegen.ts",
    "_run": "TS_NODE_PROJECT=apps/chat-app/tsconfig.app.json yarn ts-node -r tsconfig-paths/register --esm",
    "test:watch": "yarn _with-test-env vitest --watch "
  },
  "prisma": {
    "schema": "./apps/chat-app/src/infrastructure/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@apollo/server": "^4.10.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.14.0",
    "apollo-client": "^2.6.10",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "env-cmd": "^10.1.0",
    "envalid": "^8.0.0",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "ramda": "^0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "react-router-dom": "6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.14",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.9",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "19.1.1",
    "@nx/eslint-plugin": "19.1.1",
    "@nx/jest": "19.1.1",
    "@nx/js": "19.1.1",
    "@nx/nest": "19.1.1",
    "@nx/node": "19.1.1",
    "@nx/react": "19.1.1",
    "@nx/vite": "19.1.1",
    "@nx/web": "19.1.1",
    "@nx/webpack": "19.1.1",
    "@nx/workspace": "19.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@types/ramda": "^0.30.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/react-is": "18.3.0",
    "@types/styled-components": "5.1.26",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^1.3.1",
    "babel-jest": "^29.4.1",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-pathnames": "^1.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jsdom": "~22.1.0",
    "nx": "19.1.1",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "supertest": "^7.0.0",
    "supertest-graphql": "^1.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.4.2",
    "unplugin-swc": "^1.5.1",
    "vite": "~5.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.2",
    "webpack-cli": "^5.1.4"
  }
}
