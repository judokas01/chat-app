{
  "name": "@chat-app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts*\"",
    "compose:up": "docker-compose -f ./dev-docker/docker-compose.yml --profile local-mongo up --remove-orphans",
    "dev": "nx run-many -t serve",
    "code:check": "yarn _lint:check && yarn _prettier:check",
    "nest-generate": "sh -c 'yarn nx g @nx/nest:${0} ${1} --directory apps/chat-app/src/app/${1}'",
    "nest-gen:resource": "nx g @nx/nest:resource",
    "nest-gen:module": "nx g @nx/nest:module",
    "nest-gen:controller": "nx g @nx/nest:controller",
    "nest-gen:service": "nx g @nx/nest:service",
    "change-dir": "cd ./apps/chat-app/",
    "_prettier:check": "prettier --check \"apps/**/*.ts*\"",
    "_lint:check": "nx run-many --target=lint",
    "test": "nx run chat-app:test"
  },
  "prisma": {
    "schema": "./apps/chat-app/src/infrastructure/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.14.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "envalid": "^8.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "react-router-dom": "6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "19.1.1",
    "@nx/eslint-plugin": "19.1.1",
    "@nx/jest": "19.1.1",
    "@nx/js": "19.1.1",
    "@nx/nest": "19.1.1",
    "@nx/node": "19.1.1",
    "@nx/react": "19.1.1",
    "@nx/vite": "19.1.1",
    "@nx/web": "19.1.1",
